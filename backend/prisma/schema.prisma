generator client {
    provider = "prisma-client-js"
    output   = "../../node_modules/.prisma/client" // Viktig for monorepo!
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Players {
    id        Int     @id @default(autoincrement())
    name      String?
    highscore Int?
}

// Start node is the node where the player starts and is 
model CityMap {
    id   Int    @id @default(autoincrement())
    name String
}
