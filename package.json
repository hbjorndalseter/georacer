{
  "name": "georacer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "vite --config frontend/vite.config.js",
    "build": "vite build --config frontend/vite.config.js",
    "serve": "vite preview",
    "start": "node backend/index.js",
    "dev": "concurrently --kill-others-on-fail \"npm run dev:client\" \"cd backend && npm run start\"",
    "start:backend": "npx prisma db push && node backend/index.js"
  },
  "prisma": {
    "schema": "backend/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@tailwindcss/vite": "^4.0.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "georacer": "file:",
    "leaflet": "^1.9.4",
    "pg": "^8.13.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.2.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "postcss": "^8.5.3",
    "prisma": "^6.5.0",
    "tailwindcss": "^4.0.8",
    "vite": "^6.1.0"
  }
}
